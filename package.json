{
  "name": "expo-redux-devtools",
  "version": "0.0.1",
  "description": "The full Redux DevTools for Expo as a dev plugin",
  "license": "MIT",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "expo-module build",
    "clean": "expo-module clean",
    "prepare": "expo-module prepare",
    "prepublishOnly": "expo-module prepublishOnly && cd webui && npx expo export -p web --output-dir ../dist",
    "web:dev": "cd webui && npx expo start -w",
    "web:export": "cd webui && npx expo export -p web --output-dir ../dist",
    "postinstall": "cd webui && npm install"
  },
  "keywords": [
    "expo",
    "devtools"
  ],
  "files": [
    "build",
    "dist",
    "expo-module.config.json"
  ],
  "dependencies": {},
  "devDependencies": {
    "expo": "~50.0.1",
    "expo-module-scripts": "^3.1.0",
    "typescript": "^5.1.3"
  },
  "peerDependencies": {
    "expo": "*"
  }
}
